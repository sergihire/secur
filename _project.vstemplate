<?xml version="1.0" encoding="utf-8"?>
<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
  <TemplateData>
    <Name>ASP.NET Core MVC Starter Application (.NET Core)</Name>
    <Description>A basic MVC web application that makes it easy to get started.</Description>
